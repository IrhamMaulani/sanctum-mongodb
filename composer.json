{
    "name": "shahroz/sanctum-mongo",
    "description": "Laravel Sanctum provides a featherweight authentication system for SPAs and simple APIs.This will be more friendly to mongodb.",
    "authors": [
        {
            "name": "Shahroz Allauddin",
            "email": "smshahroz.allauddin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0",
        "jenssegers/mongodb": "^3.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Shahroz\\Sanctum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shahroz\\Sanctum\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shahroz\\Sanctum\\SanctumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
